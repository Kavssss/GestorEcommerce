package backend.entities.geralEntity;

import java.time.LocalDate;
import java.util.ArrayList;
import java.util.List;

import backend.entities.mercadoLivreEntity.ItemMercadoLivreEntity;
import backend.utils.CanalVenda;

public class VendaGeralEntity {
	
	private Long id;
	private LocalDate data;
	private String qtde;
	private List<ItemMercadoLivreEntity> itens = new ArrayList<>();
	private String cliente;
	private String status;
	private CanalVenda canal;
	
	public VendaGeralEntity() {
	}
	
	public VendaGeralEntity(Long id, LocalDate data, String qtde, List<ItemMercadoLivreEntity> itens, String cliente,
			String status, CanalVenda canal) {
		this.id = id;
		this.data = data;
		this.qtde = qtde;
		this.itens = itens;
		this.cliente = cliente;
		this.status = status;
		this.canal = canal;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public LocalDate getData() {
		return data;
	}

	public void setData(LocalDate data) {
		this.data = data;
	}

	public String getQtde() {
		return qtde;
	}

	public void setQtde(String qtde) {
		this.qtde = qtde;
	}

	public List<ItemMercadoLivreEntity> getItens() {
		return itens;
	}

	public void setItens(List<ItemMercadoLivreEntity> itens) {
		this.itens = itens;
	}

	public String getCliente() {
		return cliente;
	}

	public void setCliente(String cliente) {
		this.cliente = cliente;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public CanalVenda getCanal() {
		return canal;
	}

	public void setCanal(CanalVenda canal) {
		this.canal = canal;
	}
		
}
